{
    "manifest_version": 3,
    "name": "Search Assistor",
    "version": "1.0",
    "description": "Helping you to search.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon_48.png"
    },
    "icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    }
}
